{
  "name": "vscode-catlang",
  "displayName": "vscode-catlang",
  "description": "Language support for Catlang",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:catlang"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-catlang.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "catlang",
        "aliases": [
          "Catlang",
          "catlang"
        ],
        "extensions": [
          ".cat"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "catlang",
        "scopeName": "source.catlang",
        "path": "./syntaxes/catlang.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.15",
    "@types/vscode": "^1.47.0",
    "@types/which": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "which": "^2.0.2"
  }
}
